<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_login_graph"
    app:startDestination="@id/publicUserInfoFragment">
    <fragment
        android:id="@+id/publicUserInfoFragment"
        android:name="com.example.githubreporover.LoginWithGitAndGoogleFragment"
        android:label="fragment_public_user_info"
        tools:layout="@layout/fragment_login_with_git_and_google" >
        <action
            android:id="@+id/action_publicUserInfoFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.githubreporover.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>